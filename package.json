{
  "name": "setup-cpany",
  "version": "0.0.0",
  "private": true,
  "description": "Setup CPany environment",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "all": "pnpm run build && pnpm run format && pnpm run lint && pnpm run package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cpanyjs/setup-cpany.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup",
    "CPany"
  ],
  "author": "XLor",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.6.0",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.4",
    "@types/node": "^16.10.5",
    "@typescript-eslint/parser": "^5.0.0",
    "@vercel/ncc": "^0.31.1",
    "eslint": "^7.32.0",
    "eslint-plugin-github": "^4.3.2",
    "prettier": "2.4.1",
    "typescript": "^4.4.4"
  }
}
